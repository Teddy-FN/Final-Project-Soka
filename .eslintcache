\n            setLoading(false);\n            setUser(res.data.data);\n        })\n            .catch(error => {\n                console.log(error)\n            })\n    }, [user, url, loading])\n\n\n    console.log('User Info', editUser) // Line 29\n    // Edit User Profile \n    const handleChangeBio = e => {\n        setLoading(true)\n        e.preventDefault()\n        console.log('Change Profile', e.currentTarget.value);\n        setEditUser(e.currentTarget.value);\n        axios.put(`https://soka.kuyrek.com:3005/user/edit`, {\n            headers: {\n                Authorization: `Bearer ${token}`,\n                'Accept': 'application/json',\n                'Content-Type': 'application/json'\n            }\n        })\n            .then((userProfile) => {\n                setLoading(false)\n                console.log(userProfile)\n                setUser(userProfile)\n            })\n    }\n\n    return (\n        <div>\n            <div className=\"subMenu\">\n                <Container>\n                    <aside className=\"leftSide\">\n                        <div>\n                            <div className=\"profile\">\n                                <img src={`https://soka.kuyrek.com:3005/${user.image}`} alt=\"\" className=\"photo\" />\n                            </div>\n                            <h4>{user.fullname}</h4>\n                            <div className=\"desc\">\n                                <h4>Description</h4>\n                                <p>{user.description}</p>\n                            </div>\n                            <div className=\"button\">\n                                <Button color=\"link\" className=\"btn editBio\" onClick={toggle}>Edit Profile</Button>\n                                <Modal isOpen={modal} toggle={toggle} className={className}>\n                                    <ModalBody toggle={toggle} close={closeBtn}>\n                                        <h4 className=\"edit-title\">Edit Pr